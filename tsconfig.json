{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "./",
    "allowSyntheticDefaultImports": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "lib": [
      "es2015",
      "es2017",
      "dom"
    ],
    "module": "commonjs",
    "noImplicitAny": false,
    "paths": {
      "components/*": [
        "src/components/*"
      ],
      "services/*": [
        "src/services/*"
      ]
    },
    "removeComments": true,
    "sourceMap": true,
    "target": "es6"
  },
  "include": [
      "./src/**/*"
  ]
}